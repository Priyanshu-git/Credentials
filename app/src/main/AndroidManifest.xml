<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.credentials">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Credentials">
        <activity android:name=".DrawerActivity"></activity>
        <activity
            android:name=".AddEntryActivity"
            android:label="@string/string_add_entry"
            android:parentActivityName=".DrawerActivity" />
        <activity
            android:name=".WelcomeActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".MyProfile"
            android:label="@string/string_my_profile"
            android:parentActivityName=".DrawerActivity" />
        <activity
            android:name=".SplashScreen"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>